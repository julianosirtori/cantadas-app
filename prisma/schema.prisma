// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model PickUpLine {
  id     Int      @id @default(autoincrement())
  line   String
  rating Rating[]
}

model Rating {
  id           Int        @id @default(autoincrement())
  value        Int
  pickUpLine   PickUpLine @relation(fields: [pickUpLineId], references: [id])
  pickUpLineId Int
}
